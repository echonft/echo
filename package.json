{
  "name": "echo",
  "version": "1.0.0",
  "repository": "git@github.com:echonft/echo.git",
  "license": "MIT",
  "private": true,
  "description": "A Discord bot that allows NFT swapping",
  "keywords": [
    "discord",
    "bot",
    "nft"
  ],
  "bugs": {
    "url": "https://github.com/echonft/echo/issues"
  },
  "homepage": "https://github.com/echonft/echo#readme",
  "lint-staged": {
    "*.(json|yaml|cjs|js|ts|tsx)": "prettier --write --cache"
  },
  "scripts": {
    "bot:build": "turbo run build --filter=@echo/bot",
    "bot:deploy-commands": "env-cmd -f .env turbo run @echo/bot#deploy-commands",
    "bot:dev": "env-cmd -f .env turbo run dev --filter=@echo/bot... start:dev --filter=@echo/bot",
    "bot:start": "turbo run start --filter=@echo/bot",
    "build": "turbo run build --filter=@echo/frontend^... --filter=@echo/bot^... --filter=@echo/storybook^..",
    "build:fix": "turbo run build:fix",
    "lint:ci": "turbo run lint:ci",
    "prettier": "turbo run prettier",
    "test:ci": "turbo run test:ci",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "frontend:build": "turbo run build --filter=@echo/frontend",
    "frontend:dev": "env-cmd -f .env turbo run dev --filter=@echo/frontend",
    "frontend:start": "turbo run start --filter=@echo/frontend",
    "lint": "turbo run lint",
    "lint:clean-cache": "rm -f .eslintcache && turbo run lint:clean-cache",
    "modules:remove-all": "turbo run remove-modules && rm -rf node_modules",
    "modules:update-all": "ncu -u && turbo run update-modules",
    "modules:upgrade": "turbo run modules:upgrade --continue --",
    "prepare": "husky install",
    "storybook:build": "turbo run build --filter=@echo/storybook",
    "storybook:dev": "turbo run dev --filter=@echo/storybook...",
    "test": "env-cmd -f .env turbo run test",
    "test:watch": "env-cmd -f .env turbo run test",
    "tsc": "turbo run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo && turbo run tsc:clean-cache",
    "vercel": "env-cmd -f .env vercel dev"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@types/node": "18.15.3",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "env-cmd": "10.1.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-turbo": "0.0.9",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "13.2.0",
    "npm-check-updates": "16.7.12",
    "prettier": "2.8.4",
    "turbo": "1.9.3",
    "typescript": "4.9.5",
    "vercel": "28.18.5"
  },
  "packageManager": "pnpm@8.3.0",
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}
