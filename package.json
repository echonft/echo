{
  "name": "echo",
  "version": "1.0.0",
  "repository": "git@github.com:echonft/echo.git",
  "license": "MIT",
  "private": true,
  "description": "A Discord bot that allows NFT swapping",
  "keywords": [
    "discord",
    "bot",
    "nft"
  ],
  "bugs": {
    "url": "https://github.com/echonft/echo/issues"
  },
  "homepage": "https://github.com/echonft/echo#readme",
  "lint-staged": {
    "*.(json|yaml|cjs|js|ts|tsx)": "prettier --write --cache"
  },
  "scripts": {
    "bot:build": "turbo run build --filter=@echo/bot",
    "bot:deploy-commands": "env-cmd -f .env turbo run @echo/bot#deploy-commands",
    "bot:dev": "env-cmd -f .env turbo run dev --filter=@echo/bot... start:dev --filter=@echo/bot --parallel",
    "bot:start": "turbo run start --filter=@echo/bot",
    "build": "turbo run build",
    "build:dependencies": "turbo run build --filter=@echo/frontend^... --filter=@echo/bot^...",
    "ci:lint": "turbo run ci:lint --parallel",
    "ci:prettier": "turbo run ci:prettier --parallel",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel",
    "frontend:build": "turbo run build --filter=@echo/frontend",
    "frontend:dev": "env-cmd -f .env turbo run dev --filter=@echo/frontend... --parallel",
    "frontend:start": "turbo run start --filter=@echo/frontend",
    "lint": "turbo run lint --parallel",
    "lint:clean-cache": "rm -f .eslintcache && turbo run lint:cache",
    "prepare": "husky install",
    "remove-modules": "turbo run remove-modules && rm -rf node_modules",
    "tsc": "turbo run tsc --parallel",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo && turbo run tsc:clean-cache",
    "update-modules": "ncu -u && turbo run update-modules",
    "vercel": "env-cmd -f .env vercel dev"
  },
  "devDependencies": {
    "@types/node": "18.11.14",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "env-cmd": "10.1.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-turbo": "0.0.7",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "npm-check-updates": "16.5.6",
    "prettier": "2.8.1",
    "turbo": "1.6.3",
    "typescript": "4.9.4",
    "vercel": "28.8.0"
  },
  "packageManager": "pnpm@7.18.0"
}
