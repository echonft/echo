{
  "name": "echo",
  "version": "1.0.0",
  "repository": "git@github.com:echonft/echo.git",
  "license": "MIT",
  "private": true,
  "description": "A Discord bot that allows NFT swapping",
  "keywords": [
    "discord",
    "bot",
    "nft"
  ],
  "bugs": {
    "url": "https://github.com/echonft/echo/issues"
  },
  "homepage": "https://github.com/echonft/echo#readme",
  "workspaces": [
    "bot",
    "frontend",
    "lib/*"
  ],
  "lint-staged": {
    "*.(json|yaml|cjs|js|ts|tsx)": "prettier --write --cache"
  },
  "scripts": {
    "build": "turbo run build",
    "build:dependencies": "turbo run build --filter=@echo/frontend^... --filter=@echo/bot^...",
    "dev": "turbo run dev",
    "frontend:dev": "env-cmd -f .env turbo run dev --filter=@echo/frontend...",
    "frontend:build": "turbo run build --filter=@echo/frontend...",
    "frontend:build-dependencies": "turbo run build --filter=@echo/frontend^...",
    "frontend:start": "turbo run start --filter=@echo/frontend...",
    "bot:start": "turbo run start --filter@echo/bot..",
    "bot:dev": "env-cmd -f .env turbo run start:dev --filter=@echo/bot...",
    "bot:build": "turbo run build --filter=@echo/bot...",
    "bot:build-dependencies": "turbo run build --filter=@echo/bot^...",
    "bot:deploy-commands": "env-cmd -f .env turbo run @echo/bot#deploy-commands",
    "prepare": "husky install",
    "vercel": "turbo run vercel --filter=@echo/frontend^...",
    "vercel:dev": "env-cmd -f .env vercel dev",
    "lint": "turbo run lint",
    "ci:lint": "turbo run ci:lint",
    "ci:prettier": "turbo run ci:prettier",
    "tsc": "turbo run tsc",
    "clean": "turbo run clean",
    "update-modules": "ncu -u && turbo run update-packages",
    "remove-modules": "turbo run remove-modules && rm -rf node_modules"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "env-cmd": "10.1.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-turbo": "0.0.4",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "npm-check-updates": "16.4.1",
    "prettier": "2.7.1",
    "turbo": "1.6.3",
    "typescript": "4.9.3"
  }
}
