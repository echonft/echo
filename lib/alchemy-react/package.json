{
  "name": "@echo/alchemy-react",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean-bad": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "remove-modules": "rm -rf node_modules",
    "tsc": "tsc --noEmit",
    "tsc:clean-bad-build": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-modules": "ncu -u"
  },
  "peerDependencies": {
    "@mobily/ts-belt": "4.0.0-rc.5",
    "next": "13.0.6",
    "react": "18.2.0"
  },
  "dependencies": {
    "@mobily/ts-belt": "4.0.0-rc.5",
    "@echo/alchemy": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/swr": "workspace:*",
    "@echo/utils": "workspace:*",
    "alchemy-sdk": "2.6.1",
    "ramda": "0.28.0",
    "swr": "2.1.0"
  },
  "devDependencies": {
    "@types/ramda": "0.28.23",
    "@types/react": "18.0.28"
  }
}
