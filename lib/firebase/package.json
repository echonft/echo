{
  "name": "@echo/firebase",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "rm -f tsconfig.tsbuildinfo && rm -fr dist/* && tsc",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "prettier": "prettier --config --write \"src/**/*.ts\"",
    "tsc": "tsc --noEmit",
    "update-packages": "ncu -u",
    "clean": "rm -rf tsconfig.tsbuildinfo dist node_modules"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": [
      "./dist/*.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@echo/model": "workspace:*",
    "firebase": "9.14.0",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/ramda": "0.28.20",
    "typescript": "4.9.3"
  }
}
