{
  "name": "@echo/utils",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=default --reporters=jest-junit",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "5.6.0",
    "dayjs": "1.11.13",
    "pino": "9.5.0",
    "ramda": "0.30.1",
    "viem": "2.21.35",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/jsrsasign": "10.5.14",
    "@types/node": "22.8.1",
    "@types/ramda": "0.30.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.6.3"
  }
}
