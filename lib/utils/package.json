{
  "name": "@echo/utils",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "lint": "eslint --cache --fix --ext .ts src/ && eslint --cache --fix --ext .ts test/",
    "lint:ci": "eslint .ts src/ && eslint .ts test/",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -c src/ && prettier -c test/",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@jest/globals": "29.6.2",
    "@jest/types": "29.6.1",
    "@types/node": "20.4.9",
    "@types/ramda": "0.29.3",
    "dayjs": "1.11.9",
    "ethers": "6.7.0",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "pino": "8.15.0",
    "ramda": "0.29.0",
    "typescript": "5.1.6"
  }
}
