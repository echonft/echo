{
  "name": "@echo/utils",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=default --reporters=jest-junit",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "5.6.0",
    "dayjs": "1.11.12",
    "pino": "9.3.2",
    "ramda": "0.30.1",
    "viem": "2.18.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@babel/preset-typescript": "7.24.7",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/jsrsasign": "10.5.14",
    "@types/node": "22.0.2",
    "@types/ramda": "0.30.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.5.4"
  }
}
