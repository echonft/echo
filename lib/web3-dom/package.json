{
  "name": "@echo/web3-dom",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=default --reporters=jest-junit",
    "test:clean": "rimraf test/.coverage",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "exponential-backoff": "3.1.1",
    "ramda": "0.30.1",
    "siwe": "2.3.2",
    "viem": "2.21.51",
    "wagmi": "2.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "22.10.1",
    "@types/ramda": "0.30.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.7.2"
  }
}
