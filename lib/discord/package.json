{
  "name": "@echo/discord",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "remove-modules": "rm -rf node_modules",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-modules": "ncu -u"
  },
  "dependencies": {
    "@echo/api": "workspace:*",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@types/ramda": "0.28.23"
  }
}
