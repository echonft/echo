{
  "name": "@echo/api-public",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "lint": "eslint --cache --fix --ext .ts src/",
    "lint:ci": "eslint .ts src/",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -c src/",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@echo/utils": "workspace:*",
    "@jest/globals": "29.6.2",
    "@jest/types": "29.6.1",
    "@types/node": "20.4.9",
    "@types/ramda": "0.29.3",
    "@types/request": "2.48.8",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "next": "13.4.19",
    "next-auth": "4.22.5",
    "ramda": "0.29.0",
    "request": "2.88.2",
    "siwe": "2.1.4",
    "typescript": "5.1.6"
  }
}
