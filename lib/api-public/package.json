{
  "name": "@echo/api-public",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "next": "^13",
    "next-auth": "^4.20.1",
    "siwe": "^1.1.6"
  },
  "dependencies": {
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "ramda": "0.29.0",
    "request": "2.88.2"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@jest/types": "29.5.0",
    "@types/node": "18.15.3",
    "@types/ramda": "0.29.0",
    "@types/request": "2.48.8",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "node-mocks-http": "1.12.2",
    "tsup": "7.1.0",
    "typescript": "4.9.5"
  }
}
