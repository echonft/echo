{
  "name": "@echo/firestore",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "dayjs": "1.11.7",
    "ethers": "6.1.0",
    "firebase": "9.17.2",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@jest/types": "29.5.0",
    "@types/node": "18.15.3",
    "@types/ramda": "0.29.0",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "typescript": "4.9.5"
  }
}
