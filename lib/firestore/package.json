{
  "name": "@echo/firestore",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "env:pull": "dotenvx hub pull",
    "lint": "eslint",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern",
    "test": "dotenvx run --env-file=.env.test -- jest --runInBand",
    "test:coverage:clean": "rm -rf test/.coverage",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "dayjs": "1.11.11",
    "firebase-admin": "12.1.1",
    "ramda": "0.30.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@dotenvx/dotenvx": "0.44.1",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.14.2",
    "@types/ramda": "0.30.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  }
}
