{
  "name": "@echo/firestore",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean-bad": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "ci:test": "jest --ci",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "remove-modules": "rm -rf node_modules",
    "test": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-bad-build": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-modules": "ncu -u"
  },
  "dependencies": {
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "dayjs": "1.11.7",
    "firebase": "9.16.0",
    "ethers": "5.7.2",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@jest/globals": "29.4.2",
    "@jest/types": "29.4.2",
    "@types/ramda": "0.28.22",
    "babel-jest": "29.4.2",
    "firestore-jest-mock": "0.20.0",
    "jest": "29.4.2"
  }
}
