{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["dist/index.css"],
      "passThroughEnv": [
        "ANALYZE",
        "AUTH_SECRET",
        "NEXT_PUBLIC_SENTRY_DSN",
        "NEXT_PUBLIC_VERCEL_PRODUCTION_URL",
        "SECRET_MANAGER_EMAIL",
        "SECRET_MANAGER_PRIVATE_KEY",
        "SENTRY_AUTH_TOKEN",
        "SENTRY_PROJECT",
        "SENTRY_ORG",
        "VERCEL_GIT_COMMIT_SHA"
      ]
    }
  }
}
