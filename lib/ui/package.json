{
  "name": "@echo/ui",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch src/",
    "lint": "eslint",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=default --reporters=jest-junit",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@sentry/nextjs": "8.35.0",
    "@tanstack/react-query": "5.59.16",
    "connectkit": "1.8.2",
    "dayjs": "1.11.13",
    "framer-motion": "11.11.10",
    "marked-react": "2.0.0",
    "next-intl": "3.23.5",
    "prop-types": "15.8.1",
    "qs": "6.13.0",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-copy-to-clipboard": "5.1.0",
    "react-detect-click-outside": "1.1.7",
    "react-dom": "18.3.1",
    "react-loader-spinner": "6.1.6",
    "react-tooltip": "5.28.0",
    "swr": "2.2.5",
    "tailwindcss": "3.4.14",
    "throttle-debounce": "5.0.2",
