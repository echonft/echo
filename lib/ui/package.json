{
  "name": "@echo/ui",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "next": "^13",
    "react": "^18",
    "react-dom": "^18"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@types/ramda": "0.29.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "tailwindcss": "3.2.7"
  }
}
