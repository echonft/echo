{
  "name": "@echo/ui",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "remove-modules": "rm -rf node_modules",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-modules": "ncu -u"
  },
  "peerDependencies": {
    "next": "^13",
    "react": "^18"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "next": "13.2.4",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/ramda": "0.28.23",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "tailwindcss": "3.2.7",
    "tsup": "6.7.0"
  }
}
