{
  "name": "@echo/ui",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch src/",
    "lint": "eslint",
    "modules:check": "depcheck --config=\"../../.depcheckrc\"",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=default --reporters=jest-junit",
    "test:clean": "rimraf test/.coverage",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/core": "0.37.4",
    "@headlessui/react": "2.2.0",
    "@sentry/browser": "8.44.0",
    "@sentry/react": "8.44.0",
    "@tanstack/react-query": "5.62.7",
    "connectkit": "1.8.2",
    "dayjs": "1.11.13",
    "framer-motion": "11.14.1",
    "js-cookie": "3.0.5",
    "marked-react": "2.0.0",
    "next": "14.2.20",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "3.26.1",
    "prop-types": "15.8.1",
    "qs": "6.13.1",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-copy-to-clipboard": "5.1.0",
    "react-detect-click-outside": "1.1.7",
    "react-dom": "18.3.1",
    "react-loader-spinner": "6.1.6",
    "react-tooltip": "5.28.0",
    "swr": "2.2.5",
    "tailwindcss": "3.4.16",
    "throttle-debounce": "5.0.2",
    "use-long-press": "3.2.0",
    "wagmi": "2.14.1",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@echo/model": "workspace:*",
    "@echo/routing": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/js-cookie": "3.0.6",
    "@types/node": "22.10.2",
    "@types/prop-types": "15.7.14",
    "@types/qs": "6.9.17",
    "@types/ramda": "0.30.2",
    "@types/react": "18.3.16",
    "@types/react-copy-to-clipboard": "5.0.7",
    "@types/react-dom": "18.3.5",
    "@types/throttle-debounce": "5.0.2",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "cssnano": "7.0.6",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  }
}
