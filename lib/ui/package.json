{
  "name": "@echo/ui",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "next": "^13.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "0.7.7"
  },
  "dependencies": {
    "@echo/swr": "workspace:*",
    "@echo/ui-model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@headlessui/react": "1.7.16",
    "clsx": "2.0.0",
    "dayjs": "1.11.9",
    "next-intl": "2.19.1",
    "ramda": "0.29.0",
    "react-copy-to-clipboard": "5.1.0",
    "swr": "2.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.22.5",
    "@headlessui/tailwindcss": "0.2.0",
    "@jest/globals": "29.6.2",
    "@jest/types": "29.6.1",
    "@types/node": "20.4.9",
    "@types/ramda": "0.29.3",
    "@types/react": "18.2.19",
    "@types/react-copy-to-clipboard": "5.0.4",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.1",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "tailwindcss": "3.3.3",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  }
}
