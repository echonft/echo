    "@types/throttle-debounce": "5.0.2",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "cssnano": "7.0.6",
    "i18next": "23.16.4",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  }
