    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.26.0",
    "@echo/api": "workspace:*",
    "@echo/auth": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/routing": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "22.8.1",
