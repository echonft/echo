{
  "name": "@echo/ui",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch src/",
    "lint": "eslint --cache --fix --ext .ts,.tsx src/ && eslint --cache --fix --ext .ts test/",
    "lint:ci": "eslint --quiet --ext .ts,.tsx src/ && eslint --ext .ts test/",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -u -c src/ && prettier -u -c test/",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/api": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@headlessui/react": "1.7.17",
    "@sentry/nextjs": "7.77.0",
    "connectkit": "1.5.3",
    "dayjs": "1.11.10",
    "next": "14.0.1",
    "next-auth": "4.24.4",
    "next-intl": "3.0.0-beta.14",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-tooltip": "5.21.6",
    "recoil": "0.7.7",
    "siwe": "2.1.4",
    "swr": "2.2.4",
    "throttle-debounce": "5.0.0",
    "use-long-press": "3.2.0",
    "wagmi": "1.4.5",
    "zustand": "4.4.5"
  },
  "devDependencies": {
    "@babel/preset-react": "7.22.15",
    "@headlessui/tailwindcss": "0.2.0",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/ramda": "0.29.7",
    "@types/react": "18.2.33",
    "@types/react-copy-to-clipboard": "5.0.6",
    "@types/react-dom": "18.2.14",
    "@types/throttle-debounce": "5.0.1",
    "autoprefixer": "10.4.16",
    "clsx": "2.0.0",
    "cssnano": "6.0.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tailwindcss": "3.3.5",
    "tsup": "7.2.0"
  }
}
