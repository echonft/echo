{
  "name": "@echo/ui",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch src/",
    "lint": "eslint --fix -c .eslintrc.cjs --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.cjs --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c  .",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/api": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@headlessui/react": "1.7.18",
    "@sentry/nextjs": "7.93.0",
    "@tanstack/react-query": "5.18.0",
    "connectkit": "1.7.0",
    "dayjs": "1.11.10",
    "marked-react": "2.0.0",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.4",
    "next-intl": "3.4.2",
    "nextjs-router-events": "0.0.3",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-beforeunload": "2.6.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "5.4.5",
    "react-tooltip": "5.25.1",
    "siwe": "2.1.4",
    "swr": "2.2.4",
    "throttle-debounce": "5.0.0",
    "use-long-press": "3.2.0",
    "wagmi": "2.5.5",
    "zustand": "4.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.11.0",
    "@types/ramda": "0.29.9",
    "@types/react": "18.2.47",
    "@types/react-beforeunload": "2.1.5",
    "@types/react-copy-to-clipboard": "5.0.7",
    "@types/react-dom": "18.2.18",
    "@types/throttle-debounce": "5.0.2",
    "autoprefixer": "10.4.16",
    "clsx": "2.1.0",
    "cssnano": "6.0.3",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tailwindcss": "3.4.1",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
}
}
