{
  "name": "@echo/ui",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch src/",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@headlessui/react": "1.7.18",
    "@sentry/nextjs": "7.107.0",
    "@tanstack/react-query": "5.28.6",
    "connectkit": "1.7.2",
    "dayjs": "1.11.10",
    "framer-motion": "11.0.20",
    "marked-react": "2.0.0",
    "next": "14.1.4",
    "next-auth": "5.0.0-beta.15",
    "next-intl": "3.9.5",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "6.1.6",
    "react-tooltip": "5.26.3",
    "siwe": "2.1.4",
    "swr": "2.2.5",
    "throttle-debounce": "5.0.0",
    "use-long-press": "3.2.0",
    "wagmi": "2.5.12",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/preset-env": "7.24.3",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@echo/api": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.11.30",
    "@types/ramda": "0.29.11",
    "@types/react": "18.2.67",
    "@types/react-copy-to-clipboard": "5.0.7",
    "@types/react-dom": "18.2.22",
    "@types/throttle-debounce": "5.0.2",
    "autoprefixer": "10.4.19",
    "clsx": "2.1.0",
    "cssnano": "6.1.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tailwindcss": "3.4.1",
    "tsup": "8.0.2",
    "typescript": "5.4.3"
  }
}
