{
  "name": "@echo/api",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "clean": "rm -rf dist",
    "update-modules": "ncu -u",
    "remove-modules": "rm -rf node_modules"
  },
  "peerDependencies": {
    "next": "^13"
  },
  "dependencies": {
    "@echo/firebase-admin": "workspace:*",
    "@echo/firebase": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "ethers": "5.7.2",
    "iron-session": "^6.3.1",
    "rambda": "7.3.0",
    "next": "13.0.5",
    "request": "2.88.2",
    "siwe": "1.1.6"
  },
  "devDependencies": {
    "@types/request": "2.48.8"
  }
}
