{
  "name": "@echo/api",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "rm -f tsconfig.tsbuildinfo && rm -fr dist/* && tsc",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "tsc": "tsc --noEmit",
    "update-packages": "ncu -u"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": [
      "./dist/*.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@echo/firebase-admin": "workspace:*",
    "ethers": "5.7.2",
    "iron-session": "^6.3.1",
    "next": "13.0.3",
    "ramda": "0.28.0",
    "siwe": "1.1.6"
  },
  "devDependencies": {
    "@types/ramda": "0.28.20",
    "eslint-plugin-import": "2.26.0",
    "typescript": "4.9.3"
  }
}
