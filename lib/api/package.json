{
  "name": "@echo/api",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "ci:test": "jest --ci",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "remove-modules": "rm -rf node_modules",
    "test": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-modules": "ncu -u"
  },
  "peerDependencies": {
    "next": "^13"
  },
  "dependencies": {
    "@echo/alchemy": "workspace:*",
    "@echo/api-auth": "workspace:*",
    "@echo/discord": "workspace:*",
    "@echo/firebase-admin": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@mobily/ts-belt": "4.0.0-rc.5",
    "alchemy-sdk": "2.6.1",
    "ethers": "5.7.2",
    "next": "13.2.4",
    "next-auth": "4.20.1",
    "ramda": "0.28.0",
    "request": "2.88.2",
    "siwe": "1.1.6"
  },
  "devDependencies": {
    "@jest/globals": "29.4.2",
    "@jest/types": "29.4.2",
    "@types/ramda": "0.28.23",
    "@types/request": "2.48.8",
    "babel-jest": "29.4.2",
    "node-mocks-http": "1.12.2",
    "jest": "29.5.0"
  }
}
