{
  "name": "@echo/api",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "update-modules": "ncu -u",
    "remove-modules": "rm -rf node_modules"
  },
  "exports": {
    "./*": [
      "./dist/*.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "next": "13.0.4"
  },
  "dependencies": {
    "@echo/firebase-admin": "workspace:*",
    "ethers": "5.7.2",
    "iron-session": "^6.3.1",
    "next": "13.0.4",
    "ramda": "0.28.0",
    "request": "2.88.2",
    "siwe": "1.1.6"
  },
  "devDependencies": {
    "@types/ramda": "0.28.20",
    "@types/request": "2.48.8"
  }
}
