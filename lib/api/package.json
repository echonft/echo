{
  "name": "@echo/api",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.7.2",
    "path-to-regexp": "7.0.0",
    "next": "14.2.4",
    "qs": "6.12.1",
    "ramda": "0.30.1"
  },
  "devDependencies": {
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@types/node": "20.14.7",
    "@types/qs": "6.9.15",
    "@types/ramda": "0.30.0",
    "typescript": "5.5.2"
  }
}
