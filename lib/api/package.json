{
  "name": "@echo/api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "next": "^13.4.13",
    "next-auth": "^4.22.5"
  },
  "dependencies": {
    "@echo/alchemy": "workspace:*",
    "@echo/api-public": "workspace:*",
    "@echo/discord": "workspace:*",
    "@echo/firebase-admin": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "dayjs": "1.11.9",
    "ethers": "6.7.0",
    "ramda": "0.29.0",
    "request": "2.88.2",
    "siwe": "2.1.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "29.6.2",
    "@jest/types": "29.6.1",
    "@types/node": "20.4.9",
    "@types/ramda": "0.29.3",
    "@types/request": "2.48.8",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "node-mocks-http": "1.12.2",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  }
}
