{
  "name": "@echo/tasks",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest --passWithNoTests",
    "test:coverage:clean": "rm -rf test/.coverage",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "ramda": "0.30.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@echo/model": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/opensea": "workspace:*",
    "@echo/nft-scan": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.14.7",
    "@types/ramda": "0.30.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.5.2"
  }
}
