{
  "extends": "../../lib/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@echo/alchemy/*": ["../../lib/alchemy/src/*"],
      "@echo/api/*": ["../../lib/api/src/*"],
      "@echo/firestore/*": ["../../lib/firestore/src/*"],
      "@echo/firestore-mocks/*": ["../../lib/firestore/test/@mocks/*"],
      "@echo/frontend/*": ["src/*"],
      "@echo/frontend-mocks/*": ["test/@mocks/*"],
      "@echo/model/*": ["../../lib/model/src/*"],
      "@echo/model-mocks/*": ["../../lib/model/test/@mocks/*"],
      "@echo/tasks/*": ["../../lib/tasks/src/*"],
      "@echo/ui/*": ["../../lib/ui/src/*"],
      "@echo/ui-css/*": ["../../lib/ui/dist/*"],
      "@echo/utils/*": ["../../lib/utils/src/*"],
      "@echo/utils-test/*": ["../../lib/utils/test/@utils/*"],
      "@echo/web3/*": ["../../lib/web3/src/*"],
      "@echo/web3-dom/*": ["../../lib/web3-dom/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["jest"]
  },
  "include": ["src", "next-env.d.ts", ".next/types/**/*.ts", "test", "sentry.*.config.ts", "../../env.d.ts"],
  "exclude": ["node_modules", ".next"]
}
