{
  "extends": "../../lib/tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@app/*": ["src/app/*"],
      "@constants/*": ["src/lib/constants/*"],
      "@helpers/*": ["src/lib/helpers/*"],
      "@server/*": ["src/lib/server/*"],
      "@server-mocks/*": ["test/lib/server/mocks/*"],
      "@type/*": ["src/lib/types/*"],
      "@echo/alchemy/*": ["../../lib/alchemy/src/*"],
      "@echo/api/*": ["../../lib/api/src/*"],
      "@echo/discord/*": ["../../lib/discord/src/*"],
      "@echo/firestore/*": ["../../lib/firestore/src/*"],
      "@echo/firestore-mocks/*": ["../../lib/firestore/test/mocks/*"],
      "@echo/model/*": ["../../lib/model/src/*"],
      "@echo/model-mocks/*": ["../../lib/model/test/mocks/*"],
      "@echo/test-utils/*": ["../../lib/utils/test/test-utils/*"],
      "@echo/ui/*": ["../../lib/ui/src/*"],
      "@echo/ui-css/*": ["../../lib/ui/dist/*"],
      "@echo/utils/*": ["../../lib/utils/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["src", "next-env.d.ts", ".next/types/**/*.ts", "test"],
  "exclude": ["node_modules", ".next"]
}
