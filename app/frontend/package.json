{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "source .env.keys; DOTENV_KEY=$DOTENV_KEY_DEV dotenvx run -- next build",
    "build:ci": "next build",
    "clean": "rm -rf .next",
    "deploy:dev": "flyctl deploy --config .fly/fly.dev.toml --dockerfile .fly/Dockerfile",
    "dev": "source .env.keys; DOTENV_KEY=$DOTENV_KEY_DEV dotenvx run -- next dev",
    "dev:testnet": "source .env.keys; DOTENV_KEY=$DOTENV_KEY_TESTNET dotenvx run -- next dev",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org=echo-nft --project=frontend .next/ && sentry-cli sourcemaps upload --org=echo-nft --project=frontend .next/",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@auth/core": "0.28.2",
    "@sentry/integrations": "7.109.0",
    "@sentry/nextjs": "7.109.0",
    "@vercel/speed-insights": "1.0.10",
    "next": "14.1.4",
    "next-auth": "5.0.0-beta.16",
    "next-intl": "3.10.0",
    "qs": "6.12.0",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "siwe": "2.1.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "0.38.0",
    "@echo/api": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/tasks": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@sentry/cli": "2.31.0",
    "@swc/core": "1.4.11",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.2",
    "@types/qs": "6.9.14",
    "@types/ramda": "0.29.12",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.4.3"
  }
}
