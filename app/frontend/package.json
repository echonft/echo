{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next",
    "dev": "next dev | pino-pretty -c",
    "lint": "eslint",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=default --reporters=jest-junit",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@echo/api": "workspace:*",
    "@echo/auth": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/nft-scan": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/opensea": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@next/bundle-analyzer": "14.2.5",
    "@sentry/browser": "8.18.0",
    "@sentry/nextjs": "8.18.0",
    "@vercel/speed-insights": "1.0.12",
    "next": "14.2.5",
    "next-intl": "3.17.1",
    "pino": "9.3.1",
    "pino-pretty": "11.2.1",
    "qs": "6.12.3",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "siwe": "2.3.2",
    "tailwindcss": "3.4.6",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@sentry/cli": "2.32.2",
    "@swc/core": "1.6.13",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.11",
    "@types/qs": "6.9.15",
    "@types/ramda": "0.30.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.5.3"
  }
}
