    "@types/jest": "29.5.4",
    "@types/node": "20.4.9",
    "@types/ramda": "0.29.3",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "@types/react-icons": "3.0.0",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "typescript": "5.1.6"
