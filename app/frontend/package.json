{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "dotenvx run --env-file=.env.dev -- next build",
    "build:ci": "next build",
    "clean": "rm -rf .next",
    "deploy:dev": "flyctl deploy --config .fly/fly.dev.toml --dockerfile .fly/Dockerfile",
    "dev": "next dev",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org=echo-nft --project=frontend .next/ && sentry-cli sourcemaps upload --org=echo-nft --project=frontend .next/",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@auth/core": "0.26.3",
    "@headlessui/react": "1.7.18",
    "@sentry/integrations": "7.101.0",
    "@sentry/nextjs": "7.101.0",
    "@vercel/speed-insights": "1.0.9",
    "dayjs": "1.11.10",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.11",
    "next-intl": "3.7.0",
    "nextjs-router-events": "0.0.3",
    "qs": "6.11.2",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "siwe": "2.1.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "0.15.4",
    "@echo/api": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@sentry/cli": "2.28.6",
    "@swc/core": "1.4.1",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.17",
    "@types/qs": "6.9.11",
    "@types/ramda": "0.29.10",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.3.3"
  }
}
