{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next",
    "dev": "next dev",
    "lint": "eslint --cache --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -u -c src/ && prettier -u -c test/",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@auth/firebase-adapter": "1.0.3",
    "@echo/api": "workspace:*",
    "@echo/discord": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@headlessui/react": "1.7.17",
    "@sentry/integrations": "7.77.0",
    "@sentry/nextjs": "7.77.0",
    "dayjs": "1.11.10",
    "next": "14.0.2",
    "next-auth": "4.24.4",
    "next-intl": "3.0.0-beta.14",
    "qs": "6.11.2",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "siwe": "2.1.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@swc/core": "1.3.95",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.7",
    "@types/qs": "6.9.10",
    "@types/ramda": "0.29.7",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/react-icons": "3.0.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0"
  }
}
