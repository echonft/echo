{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next",
    "dev": "next dev",
    "lint": "eslint --cache --fix \"src/**/*.ts?(x)\"",
    "lint:ci": "eslint \"src/**/*.ts?(x)\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts?(x)\"",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "tsc": "tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/alchemy-react": "workspace:*",
    "@echo/api": "workspace:*",
    "@echo/discord": "workspace:*",
    "@echo/firebase-react": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/swr": "workspace:*",
    "@echo/utils": "workspace:*",
    "@headlessui/react": "1.7.13",
    "@mobily/ts-belt": "4.0.0-rc.5",
    "alchemy-sdk": "2.6.1",
    "clsx": "1.2.1",
    "connectkit": "1.2.2",
    "ethers": "5.7.2",
    "firebase": "9.17.2",
    "next": "13.2.4",
    "next-auth": "4.20.1",
    "next-intl": "2.13.1",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "siwe": "1.1.6",
    "swr": "2.1.0",
    "wagmi": "0.12.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.2.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/ramda": "0.29.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-icons": "3.0.0",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "eslint-config-next": "13.2.4",
    "jest-environment-jsdom": "^29.5.0",
    "tailwindcss": "3.2.7"
  }
