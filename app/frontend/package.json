    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@headlessui/react": "1.7.16",
    "clsx": "2.0.0",
    "dayjs": "1.11.9",
    "firebase": "10.1.0",
    "next": "13.4.19",
    "next-auth": "4.23.1",
    "next-intl": "3.0.0-beta.14",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
