{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "source .env.keys; dotenvx run -f .env.dev -- next build",
    "clean": "rm -rf .next",
    "dev": "source .env.keys; dotenvx run -f .env.dev -- next dev",
    "env:pull": "dotenvx hub pull",
    "lint": "eslint",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern",
    "sentry:sourcemaps:inject": "sentry-cli sourcemaps inject --org=echo-nft --project=frontend .next/",
    "sentry:sourcemaps:upload": "sentry-cli sourcemaps upload --org=echo-nft --project=frontend .next/",
    "start": "next start",
    "test": "jest",
    "test:coverage:clean": "rm -rf test/.coverage",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/core": "0.32.0",
    "@echo/api": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@sentry/browser": "8.7.0",
    "@sentry/nextjs": "8.7.0",
    "@vercel/speed-insights": "1.0.11",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.19",
    "next-intl": "3.14.1",
    "qs": "6.12.1",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "siwe": "2.3.2",
    "tailwindcss": "3.4.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "0.44.1",
    "@next/eslint-plugin-next": "14.2.3",
    "@sentry/cli": "2.32.1",
    "@swc/core": "1.5.24",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.1",
    "@types/qs": "6.9.15",
    "@types/ramda": "0.30.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "eslint-config-next": "14.2.3",
    "eslint-plugin-react": "7.34.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "globals": "15.3.0",
    "typescript": "5.4.5"
  }
}
