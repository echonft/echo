{
  "name": "@echo/frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "source .env.keys; dotenvx run -f .env.dev -- next build",
    "build:ci": "next build",
    "clean": "rm -rf .next",
    "deploy:dev": "flyctl deploy --config .fly/fly.dev.toml --dockerfile .fly/Dockerfile",
    "dev": "source .env.keys; dotenvx run -f .env.dev -- next dev",
    "dev:testnet": "source .env.keys; dotenvx run -f .env.testnet -- next dev",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org=echo-nft --project=frontend .next/ && sentry-cli sourcemaps upload --org=echo-nft --project=frontend .next/",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@auth/core": "0.31.0",
    "@sentry/integrations": "7.116.0",
    "@sentry/nextjs": "7.116.0",
    "@vercel/speed-insights": "1.0.11",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.18",
    "next-intl": "3.14.1",
    "qs": "6.12.1",
    "ramda": "0.30.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "siwe": "2.3.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "0.44.1",
    "@echo/api": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/tasks": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@sentry/cli": "2.31.2",
    "@swc/core": "1.5.7",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/qs": "6.9.15",
    "@types/ramda": "0.30.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "typescript": "5.4.5"
  }
}
