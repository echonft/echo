{
  "name": "@echo/firestore-functions",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "artifacts:delete": "gcloud artifacts repositories delete gcf-artifacts --location=us-central1 -q",
    "artifacts:delete:dev": "turbo artifacts:delete -- --project=echo-dev-fallback",
    "artifacts:delete:prod": "turbo artifacts:delete -- --project=echo-prod-b71e2",
    "build": "tsup",
    "deploy": "firebase deploy --only functions",
    "env:pull": "dotenvx hub pull",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts .",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern",
    "tsc": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@supercharge/promise-pool": "3.2.0",
    "dayjs": "1.11.11",
    "firebase-admin": "12.1.1",
    "firebase-functions": "5.0.1",
    "google-auth-library": "9.10.0",
    "ramda": "0.30.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/ramda": "0.30.0",
    "eslint-config-google": "0.14.0",
    "tsup": "8.1.0",
    "typescript": "5.4.5"
  }
}
