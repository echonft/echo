  "main": "dist/index.js",
  "dependencies": {
    "firebase-admin": "12.0.0",
    "firebase-functions": "4.7.0",
    "ramda": "0.29.1"
  },
  "devDependencies": {
    "@types/ramda": "0.29.9",
    "eslint-config-google": "0.14.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  }
