{
  "name": "@echo/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "nodemon --watch \"src/**\" --watch \"../../lib/**/src/**\" --ext \"ts\" --exec \"ts-node --project tsconfig.json src/index.ts\"",
    "lint": "eslint --cache --fix --ext .ts src/ && eslint --cache --fix --ext .ts test/",
    "lint:ci": "eslint --ext .ts src/ && eslint --ext .ts test/",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -u -c src/ && prettier -u -c test/",
    "start": "ts-node --project tsconfig.json src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/discord": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@sentry/node": "7.77.0",
    "@sentry/profiling-node": "1.2.6",
    "dayjs": "1.11.10",
    "discord.js": "14.13.0",
    "i18next": "23.6.0",
    "ramda": "0.29.1"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/ramda": "0.29.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0"
  }
}
