{
  "name": "@echo/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "deploy-commands": "node dist/scripts/deploy-commands.js",
    "dev": "tsup --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:ci": "eslint \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "start": "node dist/index.mjs",
    "start:dev": "nodemon --watch 'dist/*' --watch 'node_modules/@echo/*' dist/index.mjs",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@jest/globals": "29.5.0",
    "@jest/types": "29.5.0",
    "@types/node": "18.15.3",
    "@echo/api": "workspace:*",
    "@echo/discord": "workspace:*",
    "@echo/firebase-admin": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@mobily/ts-belt": "4.0.0-rc.5",
    "axios": "1.3.4",
    "dayjs": "1.11.7",
    "discord.js": "14.8.0",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@jest/types": "29.5.0",
    "@types/ramda": "0.29.0",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "nodemon": "2.0.21",
    "typescript": "4.9.5"
  }
}
