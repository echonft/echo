{
  "name": "@echo/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "build:fix": "find src/ -name \"*.js\" -type f -delete && find src/ -name \"*.d.ts*\" -type f -delete",
    "clean": "rm -rf dist",
    "deploy-commands": "node dist/scripts/deploy-commands.js",
    "dev": "nodemon --watch \"src/**\" --watch \"node_modules/@echo/**/src/**\" --ext \"ts\" --exec \"ts-node src/index.ts\"",
    "lint": "eslint --cache --fix --ext .ts src/ && eslint --cache --fix --ext .ts test/",
    "lint:ci": "eslint --ext .ts src/ && eslint --ext .ts test/",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -u -c src/ && prettier -u -c test/",
    "start": "node dist/index.js",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/discord": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/utils": "workspace:*",
    "@jest/globals": "29.6.2",
    "@jest/types": "29.6.1",
    "@types/node": "20.4.9",
    "axios": "1.4.0",
    "dayjs": "1.11.9",
    "discord.js": "14.12.1",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@echo/firestore-types": "workspace:*",
    "@jest/globals": "29.6.2",
    "@jest/types": "29.6.1",
    "@types/ramda": "0.29.3",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  }
}
