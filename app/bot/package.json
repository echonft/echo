{
  "name": "@echo/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "deploy-commands": "node dist/scripts/deploy-commands.js",
    "dev": "tsc --watch",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "remove-modules": "rm -rf node_modules",
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "start:dev": "nodemon --watch 'dist/*' --watch 'node_modules/@echo/*' --es-module-specifier-resolution=node dist/index.js",
    "tsc": "tsc --noEmit",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-modules": "ncu -u"
  },
  "dependencies": {
    "@echo/discord": "workspace:*",
    "@echo/firebase-admin": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "axios": "1.2.0",
    "discord.js": "14.7.0",
    "firebase-admin": "11.3.0",
    "rambda": "7.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
