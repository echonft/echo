{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "dotenvx run --env-file=.env -- storybook build",
    "build:ci": "storybook build",
    "clean": "rm -rf .storybook",
    "dev": "dotenvx run --env-file=.env -- storybook dev -p 6006",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@sentry/nextjs": "7.93.0",
    "@storybook/nextjs": "7.6.13",
    "dayjs": "1.11.10",
    "next-intl": "3.4.2",
    "nextjs-router-events": "0.0.3",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.6.13",
    "zustand": "4.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@dotenvx/dotenvx": "0.14.1",
    "@echo/api": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@storybook/addon-actions": "7.6.13",
    "@storybook/addon-essentials": "7.6.13",
    "@storybook/addon-styling-webpack": "0.0.6",
    "@storybook/react": "7.6.13",
    "@types/node": "20.11.0",
    "@types/ramda": "0.29.9",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "css-loader": "6.9.0",
    "eslint-plugin-storybook": "0.6.15",
    "next-auth": "5.0.0-beta.4",
    "style-loader": "3.3.4",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.3.3"
  }
}
