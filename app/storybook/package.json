{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "storybook build",
    "build:ci": "storybook build",
    "clean": "rm -rf .storybook",
    "dev": "storybook dev -p 6006",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@sentry/nextjs": "7.116.0",
    "@storybook/nextjs": "8.1.4",
    "dayjs": "1.11.11",
    "next-intl": "3.14.1",
    "ramda": "0.30.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.1.4",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.6",
    "@babel/preset-env": "7.24.6",
    "@babel/preset-react": "7.24.6",
    "@babel/preset-typescript": "7.24.6",
    "@echo/api": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@storybook/addon-actions": "8.1.4",
    "@storybook/addon-essentials": "8.1.4",
    "@storybook/addon-styling-webpack": "1.0.0",
    "@storybook/react": "8.1.4",
    "@types/node": "20.12.12",
    "@types/ramda": "0.30.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "css-loader": "6.11.0",
    "eslint-plugin-storybook": "0.8.0",
    "next-auth": "5.0.0-beta.18",
    "style-loader": "3.3.4",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.4.5"
  }
}
