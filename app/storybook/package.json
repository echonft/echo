{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "storybook build",
    "clean": "rm -rf .next",
    "dev": "storybook dev -p 6006",
    "lint": "eslint --cache --fix \"src/**/*.ts?(x)\"",
    "lint:ci": "eslint \"src/**/*.ts?(x)\"",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier --check \"src/**/*.ts?(x)\"",
    "tsc": "tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/ui": "workspace:*",
    "@storybook/addon-essentials": "7.0.4",
    "@storybook/addon-interactions": "7.0.4",
    "@storybook/addon-links": "7.0.4",
    "@storybook/addon-styling": "^1.0.0",
    "@storybook/blocks": "7.0.4",
    "@storybook/react": "7.0.4",
    "@storybook/react-webpack5": "7.0.4",
    "@storybook/testing-library": "0.0.14-next.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11"
  }
}
