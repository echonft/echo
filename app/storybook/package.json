{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "storybook build",
    "clean": "rm -rf .storybook",
    "dev": "storybook dev -p 6006",
    "lint": "eslint --cache --fix -c .eslintrc.js --ext .ts,.tsx .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts,.tsx .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c  .",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/api": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@sentry/nextjs": "7.93.0",
    "dayjs": "1.11.10",
    "net": "1.0.2",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.4",
    "next-intl": "3.4.2",
    "nextjs-router-events": "0.0.3",
    "prop-types": "15.8.1",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.6.8",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.6.8",
    "@storybook/react": "7.6.8",
    "@storybook/react-webpack5": "7.6.8",
    "@types/ramda": "0.29.9",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "eslint-plugin-storybook": "0.6.15",
    "tsconfig-paths-webpack-plugin": "4.1.0"
  }
}
