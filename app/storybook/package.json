    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "css-loader": "7.1.2",
    "eslint-plugin-storybook": "0.8.0",
    "next-auth": "5.0.0-beta.19",
    "style-loader": "4.0.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.4.5"
