    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.1.5",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.6",
    "@babel/preset-env": "7.24.6",
    "@babel/preset-react": "7.24.6",
    "@babel/preset-typescript": "7.24.6",
    "@echo/api": "workspace:*",
