{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "storybook build",
    "clean": "rm -rf .next",
    "dev": "storybook dev -p 6006",
    "lint": "eslint --cache --fix --ext .ts,.tsx src/",
    "lint:ci": "eslint --ext .ts,.tsx src/",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove-all": "rm -rf node_modules",
    "modules:update-all": "ncu -u",
    "modules:upgrade": "pnpm upgrade",
    "prettier": "prettier -u -c src/",
    "tsc": "tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@echo/api": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "browserify-zlib": "0.2.0",
    "constants-browserify": "1.0.0",
    "crypto-browserify": "3.12.0",
    "dayjs": "1.11.9",
    "next": "13.4.19",
    "next-intl": "3.0.0-beta.14",
    "path-browserify": "1.0.1",
    "prop-types": "15.8.1",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.4.0",
    "stream-browserify": "3.0.0"
    "swr": "2.2.0",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/global": "5.0.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-webpack5": "7.4.0",
    "@storybook/testing-library": "0.2.0",
    "@types/ramda": "0.29.3",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "eslint-plugin-storybook": "0.6.13",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.1.6"
  }
}
