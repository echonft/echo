{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "storybook build --test",
    "clean": "rimraf storybook-static",
    "dev": "storybook dev -p 6006",
    "lint": "eslint",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@sentry/nextjs": "8.35.0",
    "@storybook/nextjs": "8.3.6",
    "dayjs": "1.11.13",
    "next-intl": "3.23.5",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.3.6",
    "zustand": "5.0.0"
  },
  "devDependencies": {
    "@echo/api": "workspace:*",
    "@echo/auth": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@storybook/addon-actions": "8.3.6",
    "@storybook/addon-essentials": "8.3.6",
    "@storybook/addon-styling-webpack": "1.0.0",
    "@storybook/react": "8.3.6",
    "@types/node": "22.8.1",
    "@types/ramda": "0.30.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "css-loader": "7.1.2",
    "style-loader": "4.0.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.6.3"
  }
}
