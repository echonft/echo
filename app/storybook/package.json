{
  "name": "@echo/storybook",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "eslint",
    "modules:check": "depcheck --config=\"../../.depcheckrc\"",
    "modules:remove": "rimraf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@sentry/nextjs": "8.42.0",
    "dayjs": "1.11.13",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "3.25.3",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.4.7",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@echo/model": "workspace:*",
    "@echo/ui": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3-dom": "workspace:*",
    "@storybook/addon-actions": "8.4.7",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-webpack5-compiler-swc": "1.0.5",
    "@storybook/nextjs": "8.4.7",
    "@storybook/react": "8.4.7",
    "@types/node": "22.10.1",
    "@types/ramda": "0.30.2",
    "@types/react": "18.3.14",
    "@types/react-dom": "18.3.2",
    "typescript": "5.7.2"
  }
}
