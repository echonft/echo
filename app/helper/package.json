{
  "name": "@echo/helper",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "db:reset:dev": "ts-node --project tsconfig.json src/scripts/reset-db-dev.ts",
    "db:reset:test": "ts-node --project tsconfig.json src/scripts/reset-db-test.ts",
    "add-collection": "dotenvx run --env-file=../../.env.dev -- ts-node --project tsconfig.json src/scripts/add-collection-script.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --watch \"src/**\" --watch \"../../lib/**/src/**\" --ext \"ts\" --exec \"ts-node --project tsconfig.json src/index.ts\"",
    "lint": "eslint --fix -c .eslintrc.cjs --ext .ts .",
    "lint:ci": "eslint --quiet -c .eslintrc.cjs --ext .ts .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c  .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org echo-nft --project helper dist/ && sentry-cli sourcemaps upload --org echo-nft --project helper dist/",
    "start": "export NODE_ENV=production; ts-node --project tsconfig.json src/index.ts",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --no-cache --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo",
    "update-db": "export NODE_ENV=production; ts-node --project tsconfig.json src/scripts/update-db-script.ts"
  },
  "dependencies": {
    "@echo/alchemy": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@sentry/node": "7.93.0",
    "@sentry/profiling-node": "1.3.5",
    "cron": "3.1.6",
    "dayjs": "1.11.10",
    "ramda": "0.29.1"
  },
  "devDependencies": {
  "@babel/core": "7.23.7",
  "@babel/preset-env": "7.23.8",
  "@babel/preset-typescript": "7.23.3",
  "@jest/globals": "29.7.0",
  "@jest/types": "29.6.3",
  "@types/node": "20.11.0",
  "@types/ramda": "0.29.9",
  "@types/yargs": "17.0.32",
  "jest": "29.7.0",
  "jest-junit": "16.0.0",
  "nodemon": "3.0.2",
  "ts-node": "10.9.2",
  "tsconfig-paths": "4.2.0",
  "typescript": "5.3.3",
  "yargs": "17.7.2"
}
}
