{
  "extends": "../../lib/tsconfig/tsconfig.dom.json",
  "ts-node": {
    "require": ["tsconfig-paths/register"],
    "transpileOnly": true,
    "compilerOptions": {
      "moduleResolution": "node",
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@echo/alchemy/*": ["../../lib/alchemy/src/*"],
      "@echo/api/*": ["../../lib/api/src/*"],
      "@echo/commands/*": ["./src/*"],
      "@echo/model/*": ["../../lib/model/src/*"],
      "@echo/firestore/*": ["../../lib/firestore/src/*"],
      "@echo/opensea/*": ["../../lib/opensea/src/*"],
      "@echo/tasks/*": ["../../lib/tasks/src/*"],
      "@echo/utils/*": ["../../lib/utils/src/*"],
      "@echo/web3/*": ["../../lib/web3/src/*"]
    },
    "noEmit": false,
    "sourceMap": true,
    "inlineSources": true
  },
  "include": ["src", "../../env.d.ts"],
  "exclude": ["dist", "node_modules"]
}
