{
  "name": "@echo/commands",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore --config ../../prettier.config.js",
    "start": "ENV=staging tsx src/index.ts -c=update-users-nfts | pino-pretty -c",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@echo/api": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/nft-scan": "workspace:*",
    "@echo/opensea": "workspace:*",
    "@echo/tasks": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@types/node": "20.14.7",
    "@types/qs": "6.9.15",
    "@types/ramda": "0.30.0",
    "@types/yargs": "17.0.32",
    "axios": "1.7.2",
    "pino": "9.2.0",
    "pino-pretty": "11.2.1",
    "qs": "6.12.1",
    "ramda": "0.30.1",
    "tsx": "4.15.7",
    "typescript": "5.5.2",
    "yargs": "17.7.2"
  }
}
