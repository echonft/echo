{
  "name": "@echo/commands",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "command:add-collection:dev": "dotenvx run --env-file=.env.dev -- ts-node --project tsconfig.json src/commands/add-collection-command.ts",
    "command:add-collection:testnet": "dotenvx run --env-file=.env.testnet -- ts-node --project tsconfig.json src/commands/add-collection-command.ts",
    "command:add-collections:dev": "dotenvx run --env-file=.env.dev -- ts-node --project tsconfig.json src/commands/add-collections-command.ts",
    "command:add-collections:testnet": "dotenvx run --env-file=.env.testnet -- ts-node --project tsconfig.json src/commands/add-collections-command.ts",
    "command:add-collection-guild:dev": "dotenvx run --env-file=.env.dev -- ts-node --project tsconfig.json src/commands/add-collection-guild-command.ts",
    "command:add-collection-guild:testnet": "dotenvx run --env-file=.env.testnet -- ts-node --project tsconfig.json src/commands/add-collection-guild-command.ts",
    "command:clear-db:dev": "dotenvx run --env-file=.env.dev -- ts-node --project tsconfig.json src/commands/clear-db-command.ts",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "ramda": "0.29.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "0.14.1",
    "@echo/alchemy": "workspace:*",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@types/node": "20.11.0",
    "@types/ramda": "0.29.10",
    "@types/yargs": "17.0.32",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
