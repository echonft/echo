{
  "name": "@echo/contract-listener",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "deploy:dev": ".fly/deploy.sh dev",
    "deploy:prod": ".fly/deploy.sh prod",
    "dev": "source .env.keys; dotenvx run --env-file=.env.dev -- tsx watch src/index.ts",
    "env:pull": "dotenvx hub pull",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts .",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "ramda": "0.30.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.24.6",
    "@babel/preset-env": "7.24.6",
    "@babel/preset-typescript": "7.24.6",
    "@dotenvx/dotenvx": "0.44.1",
    "@echo/firestore": "workspace:*",
    "@echo/nft-scan": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/opensea": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.14.1",
    "@types/ramda": "0.30.0",
    "@types/ws": "8.5.10",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tsx": "4.11.2",
    "tsup": "8.1.0",
    "typescript": "5.4.5"
  }
}
