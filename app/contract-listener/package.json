{
  "name": "@echo/contract-listener",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "deploy": ".fly/deploy.sh",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint",
    "machine:restart": ".fly/restart-machine.sh",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier . -u -c --no-error-on-unmatched-pattern",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage:clean": "rm -rf test/.coverage",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "ramda": "0.30.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@echo/firestore": "workspace:*",
    "@echo/nft-scan": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/opensea": "workspace:*",
    "@echo/utils": "workspace:*",
    "@echo/web3": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.14.2",
    "@types/ramda": "0.30.0",
    "@types/ws": "8.5.10",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "tsx": "4.13.2",
    "tsup": "8.1.0",
    "typescript": "5.4.5"
  }
}
