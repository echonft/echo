{
  "name": "@echo/opensea-stream",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "DOTENV_KEY=$DOTENV_KEY_DEV dotenvx run -- tsup",
    "build:ci": "tsup",
    "clean": "rm -rf dist",
    "dev": "nodemon --watch \"src/**\" --watch \"../../lib/**/src/**\" --ext \"ts\" --exec \"DOTENV_KEY=$DOTENV_KEY_DEV dotenvx run -- ts-node --project tsconfig.json src/index.ts\"",
    "lint": "eslint --fix -c .eslintrc.js --ext .ts .",
    "lint:ci": "eslint --quiet -c .eslintrc.js --ext .ts .",
    "lint:clean-cache": "rm -f .eslintcache",
    "modules:remove": "rm -rf node_modules",
    "modules:update": "ncu -u",
    "modules:update-minor": "ncu -u --target=minor",
    "prettier": "prettier -u --no-error-on-unmatched-pattern --write --cache .",
    "prettier:ci": "prettier -u --no-error-on-unmatched-pattern --log-level=silent -c .",
    "start": "node dist/index.js",
    "test": "jest",
    "test:ci": "jest --ci --no-cache",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "tsc:ci": "pnpm run tsc",
    "tsc:clean-cache": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@opensea/stream-js": "0.1.3",
    "ramda": "0.29.1",
    "ws": "8.17.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/preset-env": "7.24.3",
    "@babel/preset-typescript": "7.24.1",
    "@dotenvx/dotenvx": "0.38.0",
    "@echo/firestore": "workspace:*",
    "@echo/model": "workspace:*",
    "@echo/utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@types/node": "20.12.2",
    "@types/ramda": "0.29.12",
    "@types/ws": "8.5.10",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "nodemon": "3.1.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.0.2",
    "typescript": "5.4.3"
  }
}
