{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "passThroughEnv": ["CI", "ENV"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "passThroughEnv": ["ENV", "NODE_ENV", "NEXT_PUBLIC_IS_TESTNET"],
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "modules:remove": {
      "cache": false
    },
    "modules:update": {
      "cache": false
    },
    "modules:update-minor": {
      "cache": false
    },
    "prettier": {},
    "test:coverage:clean": {
      "cache": false
    },
    "test": {},
    "tsc": {}
  }
}
