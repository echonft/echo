{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "passThroughEnv": ["CI", "ENV"]
    },
    "build:local": {
      "cache": false,
      "passThroughEnv": ["ANALYZE", "ENV"]
    },
    "clean": {
      "cache": false
    },
    "deploy": {
      "passThroughEnv": ["ENV"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "machine:restart": {
      "cache": false
    },
    "modules:remove": {
      "cache": false
    },
    "modules:update": {
      "cache": false
    },
    "modules:update-minor": {
      "cache": false
    },
    "prettier": {},
    "start": {
      "cache": false,
      "persistent": true
    },
    "start:local": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["ENV"]
    },
    "sentry:sourcemaps:inject": {
      "dependsOn": ["build"]
    },
    "sentry:sourcemaps:upload": {
      "dependsOn": ["sentry:sourcemaps:inject"]
    },
    "test:coverage:clean": {
      "cache": false
    },
    "test": {},
    "tsc": {}
  }
}
