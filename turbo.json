{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "inputs": ["src/**/*.ts", "src/**/*.tsx"]
    },
    "build:fix": {
      "inputs": ["src/**/*.d.ts", "src/**/*.js"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", ".eslintrc.js", ".eslintrc.cjs"]
    },
    "lint:ci": {
      "cache": false
    },
    "lint:clean-cache": {
      "inputs": [".eslintcache"]
    },
    "modules:remove-all": {
      "cache": false
    },
    "modules:update-all": {
      "cache": false
    },
    "modules:upgrade": {
      "cache": false
    },
    "prettier": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"]
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": [".next/**", "dist/**"]
    },
    "test": {
      "inputs": ["jest.config.*", "src/**/*.ts", "test/**/*.ts", "**/*.test.*"]
    },
    "test:ci": {
      "cache": false
    },
    "test:watch": {
      "cache": false
    },
    "tsc": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"]
    },
    "tsc:clean-cache": {
      "inputs": ["tsconfig.tsbuildinfo"]
    },
    "@echo/bot#build": {
      "dependsOn": ["@echo/ui#build"],
      "env": ["BASE_URL", "DISCORD_CLIENT_TOKEN", "DISCORD_CLIENT_ID", "DISCORD_CLIENT_SECRET", "DISCORD_REDIRECT_URI"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts"]
    },
    "@echo/bot#dev": {
      "dependsOn": ["@echo/ui#build"],
      "cache": false
    },
    "@echo/bot#deploy-commands": {
      "cache": false
    },
    "@echo/frontend#build": {
      "dependsOn": ["@echo/ui#build"],
      "env": [
        "ALCHEMY_API_KEY",
        "NODE_ENV",
        "NEXTAUTH_URL_INTERNAL",
        "NEXTAUTH_URL",
        "ADMIN_API_KEY",
        "DISCORD_CLIENT_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_REDIRECT_URI"
      ],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "@echo/frontend#dev": {
      "dependsOn": ["@echo/ui#build"],
      "cache": false
    },
    "@echo/storybook#build": {
      "dependsOn": ["@echo/ui#build"],
      "env": ["ALCHEMY_API_KEY", "STORYBOOK_ALCHEMY_API_KEY"],
      "outputs": ["storybook-static/**", "dist/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "@echo/storybook#dev": {
      "dependsOn": ["@echo/ui#build"],
      "cache": false
    },
    "@echo/ui#build": {
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "//#vercel:dev": {
      "dependsOn": ["@echo/ui#build"],
      "cache": false
    }
  }
}
