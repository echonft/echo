{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalPassThroughEnv": ["CI", "ENV", "LOG_LEVEL", "NODE_ENV"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "emulator": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "modules:remove": {
      "cache": false
    },
    "modules:update": {
      "cache": false
    },
    "modules:update-minor": {
      "cache": false
    },
    "prettier": {},
    "start": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "test:ci": {},
    "test:clean": {
      "cache": false
    },
    "tsc": {}
  }
}
