{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "prettier": {
      "outputs": []
    },
    "tsc": {
      "outputs": []
    },
    "update-packages": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "inputs": [".env"]
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "@echo/bot#deploy-commands": {
      "cache": false,
      "dependsOn": ["@echo/discord#build"],
      "outputs": ["dist/**"],
      "inputs": [".env"]
    }
  },
  "globalDependencies": ["tsconfig.json"]
}
