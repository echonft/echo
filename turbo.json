{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "ci:lint": {
      "cache": false
    },
    "ci:prettier": {
      "cache": false
    },
    "ci:test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "lint:clean-cache": {
      "cache": false
    },
    "remove-modules": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "start:dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "tsc": {
      "cache": false
    },
    "tsc:clean-cache": {
      "cache": false
    },
    "update-modules": {
      "cache": false
    },
    "@echo/api#build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV", "API_ENV", "NEXTAUTH_URL_INTERNAL", "NEXTAUTH_URL"],
      "outputs": ["dist/**"]
    },
    "@echo/discord#build": {
      "dependsOn": ["^build"],
      "env": [
        "DISCORD_CLIENT_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_REDIRECT_URI",
        "DISCORD_GUILD_ID",
        "DISCORD_APP_ENV"
      ],
      "outputs": ["dist/**"]
    },
    "@echo/firebase-admin#build": {
      "dependsOn": ["^build"],
      "env": ["FIREBASE_ADMIN_PRIVATE_KEY"],
      "outputs": ["dist/**"]
    },
    "@echo/frontend#build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_CHAIN_ID",
        "NEXT_PUBLIC_MOCK",
        "NEXT_PUBLIC_ALCHEMY_API_KEY_MAINNET",
        "NEXT_PUBLIC_ALCHEMY_API_KEY_GOERLI"
      ],
      "outputs": [".next/**", "dist/**"]
    },
    "@echo/storybook#build": {
      "dependsOn": ["^build"],
      "outputs": ["storybook-static/**", "dist/**"]
    },
    "@echo/utils#build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV"],
      "outputs": ["dist/**"]
    },
    "@echo/bot#deploy-commands": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"],
      "inputs": [".env"]
    }
  },
  "globalDependencies": [".env", ".eslintrc.js", "prettier.config.js", "tsconfig.json"]
}
