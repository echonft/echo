{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"]
    },
    "build:fix": {
      "inputs": ["src/**/*.d.ts", "src/**/*.js"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", ".eslintrc.js", ".eslintrc.cjs"]
    },
    "lint:ci": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint:clean-cache": {
      "inputs": [".eslintcache"]
    },
    "modules:remove-all": {
      "cache": false
    },
    "modules:update-all": {
      "cache": false
    },
    "modules:upgrade": {
      "cache": false
    },
    "prettier": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "start:dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["jest.config.*", "**/*.test.*"]
    },
    "test:ci": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "tsc": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"]
    },
    "tsc:clean-cache": {
      "inputs": ["tsconfig.tsbuildinfo"]
    },
    "@echo/alchemy#build": {
      "dependsOn": ["^build"],
      "env": ["ALCHEMY_API_KEY", "NODE_ENV"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts"]
    },
    "@echo/api#build": {
      "dependsOn": ["^build"],
      "env": ["NEXTAUTH_URL_INTERNAL", "NEXTAUTH_URL", "ADMIN_API_KEY"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts"]
    },
    "@echo/api-public#build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_API_URL"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts"]
    },
    "@echo/bot#build": {
      "dependsOn": ["^build"],
      "env": ["BASE_URL"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts"]
    },
    "@echo/bot#deploy-commands": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "@echo/discord#build": {
      "dependsOn": ["^build"],
      "env": [
        "DISCORD_CLIENT_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_REDIRECT_URI",
        "DISCORD_APP_ENV"
      ],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    },
    "@echo/firestore#build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    },
    "@echo/frontend#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "@echo/storybook#build": {
      "dependsOn": ["^build"],
      "env": ["ALCHEMY_API_KEY", "STORYBOOK_ALCHEMY_API_KEY"],
      "outputs": ["storybook-static/**", "dist/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "@echo/utils#build": {
      "dependsOn": ["^build"],
      "env": ["NODE_ENV"],
      "outputs": ["dist/**"],
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    }
  }
}
