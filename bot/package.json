{
  "name": "@echo/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "build": "rm -f tsconfig.tsbuildinfo && rm -fr dist/* && tsc",
    "deploy-commands": "ts-node-esm src/scripts/deploy-commands.ts",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "lint": "eslint --fix \"src/**/*.ts\"",
    "tsc": "tsc --noEmit",
    "update-packages": "ncu -u",
    "clean": "rm -rf tsconfig.tsbuildinfo dist node_modules"
  },
  "dependencies": {
    "@echo/discord": "workspace:*",
    "@echo/firebase-admin": "workspace:*",
    "@echo/model": "workspace:*",
    "axios": "1.1.3",
    "discord.js": "14.6.0",
    "firebase-admin": "11.2.1",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/ramda": "0.28.20",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  }
}
