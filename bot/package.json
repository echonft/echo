{
  "name": "@echo/bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "pnpm build --watch",
    "start:dev": "nodemon dist/index.js",
    "build": "tsc",
    "deploy-commands": "node dist/scripts/deploy-commands.js",
    "ci:lint": "eslint \"src/**/*.ts\"",
    "ci:prettier": "prettier \"src/**/*.ts\"",
    "lint": "eslint --cache --fix \"src/**/*.ts\"",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "update-modules": "ncu -u",
    "remove-modules": "rm -rf node_modules"
  },
  "dependencies": {
    "@echo/discord": "workspace:*",
    "@echo/firebase-admin": "workspace:*",
    "@echo/model": "workspace:*",
    "axios": "1.1.3",
    "discord.js": "14.6.0",
    "firebase-admin": "11.2.1",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@types/ramda": "0.28.20",
    "nodemon": "^2.0.20"
  }
}
